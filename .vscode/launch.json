{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--source",
                "sample/IMG_1187.MP4",
                "--model",
                "models/ssdlite_mobiledet_coco_qat_postprocess_edgetpu.tflite",
                "--labels",
                "models/coco_labels.txt",
                "--threshold",
                "0.2",
                "--filter",
                "surfboard,person",
                "--print",
                "--loop"
            ]
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "--config=/home/leo/darknet/cfg/yolov3.cfg",
                "--model=/home/leo/darknet/yolov3.weights",
                "--classes=models/object_detection_classes_yolov3.txt",
                "--width=416",
                "--height=416",
                "--scale=0.00392",
                "--thr=0.2",
                "--input=sample/IMG_1187.MP4",
                "--rgb"
            ],
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    // add the directory where our target was built to the PATHs
                    // it gets resolved by CMake Tools:
                    "name": "PATH",
                    "value": "$PATH:${command:cmake.launchTargetDirectory}"
                }
            ],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}